{
  "name": "my-react-js-tutorial",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "env-cmd -f .env webpack-dev-server --mode development --hot",
    "build": "webpack --mode production",
    "build-from-local-env": "env-cmd -f .env.production webpack --mode production",
    "calc": "ts-node src/separateLessons/lesson2",
    "loki:update": "npm run build-storybook && loki update --requireReference --reactUri file:./storybook-static",
    "loki:approve": "loki approve",
    "lint-staged": "lint-staged",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "check": "npm run lint && npm test && npm run test:loki",
    "test:loki": "npm run build-storybook && loki --requireReference --reactUri file:./storybook-static",
    "test": "jest",
    "coverage": "jest --coverage",
    "e2e": "npx jest -c jest.e2e.config.js e2e",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --fix ./",
    "fix-style": "prettier --config .prettierrc --list-different src/**/*.{ts,scss} --write",
    "pre-commit-check": "npm run test && npm run e2e && npm run test:loki",
    "predeploy": "npm run build-from-local-env",
    "deploy": "gh-pages -d dist"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged && npm run pre-commit-check"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --ext .ts,.tsx --quiet --fix",
      "prettier --config .prettierrc --list-different --write",
      "git add ."
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/avpeshkov/my-react-js-tutorial"
  },
  "author": "Aleksandr Peshkov",
  "license": "ISC",
  "homepage": "https://avpeshkov.github.io/my-react-js-tutorial/",
