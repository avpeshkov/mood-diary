{
  "name": "my-react-js-tutorial",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "env-cmd -f .env webpack-dev-server --mode development --open --hot",
    "build": "env-cmd --no-override --silent -f .env.production webpack --mode production",
    "calc": "ts-node src/lesson2",
    "loki:update": "npm run build-storybook && loki update --requireReference --reactUri file:./storybook-static",
    "loki:approve": "loki approve",
    "lint-staged": "lint-staged",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "check": "npm run lint && npm test && npm run test:loki",
    "test:loki": "npm run build-storybook && loki --requireReference --reactUri file:./storybook-static",
    "test": "jest",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --fix ./",
    "fix-style": "prettier --config .prettierrc --list-different src/**/*.{ts,scss} --write",
    "pre-commit-check": "npm run fix-style && npm run lint && npm test && npm run test:loki && git add .",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run pre-commit-check"
    ],
    "*.scss": [
      "npm run pre-commit-check"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/avpeshkov/my-react-js-tutorial.git"
  },
  "author": "Aleksandr Peshkov",
  "license": "ISC",
  "homepage": "https://avpeshkov.github.io/my-react-js-tutorial/",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-transform-arrow-functions": "^7.12.1",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-docs": "^5.3.18",
    "@storybook/addon-knobs": "^5.3.18",
    "@storybook/addon-links": "^5.3.18",
    "@storybook/addon-storysource": "^5.3.18",
    "@storybook/addons": "^5.3.18",
    "@storybook/react": "^5.3.18",
    "@testing-library/dom": "^7.29.0",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.1.2",
    "@testing-library/react-hooks": "^3.4.2",
    "@testing-library/user-event": "^12.6.0",
    "@types/enzyme": "^3.10.5",
    "@types/jest": "^25.1.4",
    "@types/ramda": "^0.27.6",
    "@types/react": "^16.9.33",
    "@types/react-datepicker": "^3.1.1",
    "@types/react-dom": "^16.9.6",
    "@types/react-modal": "^3.10.6",
    "@types/react-router": "^5.1.8",
    "@types/react-router-dom": "^5.1.6",
    "@types/react-test-renderer": "^16.9.3",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "babel-jest": "^25.2.4",
    "babel-loader": "^8.1.0",
    "babel-plugin-emotion": "^10.0.33",
    "css-loader": "^3.5.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "gh-pages": "^3.1.0",
    "html-webpack-plugin": "^4.0.3",
    "husky": "^4.3.0",
    "jest": "^25.2.4",
    "jest-emotion": "^10.0.32",
    "lint-staged": "^10.4.2",
    "loki": "^0.27.0",
    "prettier": "^2.0.2",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-test-renderer": "^16.13.1",
    "snapshot-diff": "^0.8.1",
    "storybook-addon-react-docgen": "^1.2.32",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "worker-plugin": "^5.0.0"
  },
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "@emotion/css": "^11.0.0",
    "@emotion/styled": "^10.0.27",
    "antd": "^4.9.0",
    "babel-plugin-inline-dotenv": "^1.6.0",
    "env-cmd": "^10.1.0",
    "firebase": "^8.1.1",
    "formik": "^2.2.5",
    "ramda": "^0.27.0",
    "react": "^16.13.1",
    "react-datepicker": "^3.3.0",
    "react-dom": "^16.13.1",
    "react-modal": "^3.11.2",
    "react-router-dom": "^5.2.0"
  },
  "loki": {
    "configurations": {
      "chrome.laptop": {
        "target": "chrome.docker",
        "width": 1366,
        "height": 768,
        "deviceScaleFactor": 1,
        "mobile": false
      },
      "chrome.iphone7": {
        "target": "chrome.docker",
        "preset": "iPhone 7"
      }
    }
  }
}
